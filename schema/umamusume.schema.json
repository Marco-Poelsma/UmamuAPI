{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$data": "/data/umamusume.data.json",
  "title": "Umamusume",
  "type": "object",
  "properties": {
    "id": {
      "type": "integer",
      "autoIncrement": true,
      "primaryKey": true,
      "nullable": false
    },
    "name": {
      "type": "string",
      "autoIncrement": false,
      "primaryKey": false,
      "nullable": false
    },
    "sparks": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "spark": {
            "type": "object",
            "$ref": "spark.schema.json/spark",
            "nullable": false
          },
          "rarity": {
            "type": "integer",
            "nullable": false
          }
        }
      },
      "autoIncrement": false,
      "primaryKey": false,
      "nullable": false
    },
    "inspiration_1": {
      "type": "integer",
      "autoIncrement": false,
      "primaryKey": false,
      "nullable": false
    },
    "inspiration_2": {
      "type": "integer",
      "autoIncrement": false,
      "primaryKey": false,
      "nullable": false
    }
  },
  "required": ["id", "name", "sparks", "inspiration_1", "inspiration_2", "favourite"],
  "additionalProperties": false
}

 
